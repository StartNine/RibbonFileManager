<Application x:Class="RibbonFileManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RibbonFileManager"
             xmlns:sys="clr-namespace:System;assembly=System.Runtime"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/RibbonFileManager;component/Shale.xaml"/>
                <!--ResourceDictionary Source="pack://application:,,,/RibbonFileManager;component/LightPlexBlue.xaml"/-->
                <!--ResourceDictionary Source="pack://application:,,,/RibbonFileManager;component/DarkPlexBlue.xaml"/-->
                <!--ResourceDictionary Source="pack://application:,,,/RibbonFileManager;component/Classic.xaml"/-->
            </ResourceDictionary.MergedDictionaries>
            <sys:String x:Key="RibbonFileManagerPartition">RibbonFileManager</sys:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>